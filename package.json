					"group": "inline"
				},
				{
					"command": "databricksClusterItem.unpin",
					"when": "view == databricksClusters && viewItem =~ /.*,PINNED,.*/ && viewItem =~ /.*,MANUAL,.*/",
					"group": "inline"
				},
				{
					"command": "databricksClusterItem.useForSQL",
